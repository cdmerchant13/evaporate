{
  "name": "evaporate",
  "version": "1.0.0",
  "description": "Evaporate is a secure, ephemeral file sharing tool built entirely on the Cloudflare serverless ecosystem. Upload files, generate a unique link, and have them automatically \"evaporate\" after a set time or a single download.",
  "main": "index.js",
  "scripts": {
    "install:worker": "cd worker && npm install",
    "install:frontend": "cd frontend && npm install",
    "install": "npm-run-all install:*",
    "dev:worker": "cd worker && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "dev": "npm-run-all --parallel dev:*",
    "deploy:worker": "cd worker && npm run deploy",
    "build:frontend": "cd frontend && npm run build",
    "deploy": "npm run deploy:worker && npm run build:frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdmerchant13/evaporate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cdmerchant13/evaporate/issues"
  },
  "homepage": "https://github.com/cdmerchant13/evaporate#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
